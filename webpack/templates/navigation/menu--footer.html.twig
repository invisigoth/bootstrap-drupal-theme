<ul class="d-flex gap-3 gap-md-4 gap-xl-5 flex-row flex-wrap list-unstyled justify-content-center small">
	{% for item in items %}


		{% set title %}
		{{ item.title -}}{% if item.url.external %}<i class="fal fa-external-link ms-2"></i>{% endif %}
		{% endset %}
		<li>{{ link(title, item.url, {'class': link_classes}) }}</li>
	{% endfor %}
</ul>
